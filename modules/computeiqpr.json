    ,"fields"   : [
        {
            "role"       : "input"
            ,"id"        : "module_header"
            ,"label"     : "Compute I(q)/P(r), preselect models"
            ,"type"      : "label"
            ,"default"   : "header4"
            ,"prehline"  : "true"
            ,"posthline" : "true"
            ,"layout" : {
                "parent" : "header"
            }
        }
        ,{
            "role"              : "output"
            ,"id"               : "pname"
            ,"label"            : "Project name"
            ,"type"             : "text"
            ,"required"         : "true"
            ,"help"             : "Name for this project - only letters, numbers and underscores are allowed"
            ,"pattern"          : "[a-zA-Z0-9_]+"
            ,"specifiedproject" : ""
            ,"layout"     : {
                "parent" : "inputpanel"
            }

        }
        ,{
            "role"              : "output"
            ,"id"               : "desc"
            ,"label"            : "Description"
            ,"type"             : "textarea"
            ,"required"         : "true"
            ,"help"             : "Description information about this project"
            ,"layout"     : {
                "parent" : "inputpanel"
            }
        }
        ,{
            "label" : "Downloads",
            "role" : "output",
            "help" : "",
            "id" : "downloads",
            "type" : "html",
            "labelmargintop" : "0.5rem"
            ,"layout" : {
                "parent" : "resultpanel"
            }
        }
        ,{
            "role"              : "input"
